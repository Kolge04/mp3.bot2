{
  "name": "𝐊 𝐎 𝐋 𝐆 Ə _ 𝐌 𝐏 3 🎧 ",
  "description": "Telegram qruplarınızda səsli yayın vasdəsi ilə musiqi dinləyə bilərsiz.",
  "keywords": ["music", "voicechat", "telegram"],
  "repository": "https://github.com/Kolge04/mp3.bot2",
  "stack": "container",
  "env": {
    "SESSION_NAME": {
      "description": "Pyrogram Session i daxil edin (@StarkStringGenBot)",
      "required": true
    },
    "BOT_TOKEN": {
      "description": "Bot TOKEN in daxil edin (@botfather).",
      "required": true
    },
    "BOT_NAME": {
      "description": "Musiqi Bot Adınız.",
      "required": false,
      "value": ""
    },
    "BOT_USARNAME": {
      "description": "Musiqi Oynatıcı Bot tağ adlnı daxil edin (@ olmadan yazın).",
      "required": true,
      "value": ""
     
    },
    "API_ID": {
      "description": "APİ_ID i daxil edin (my.telegram.org).",
      "required": true
    },
    "API_HASH": {
      "description": "APİ_HASH ı daxil edin (my.telegram.org)",
      "required": true
    },
    "SUDO_USERS": {
      "description": "Her yrdə yönətici olaraq sayılan ASİSTAN ın İD in daxil edin  (ASİSTAN hesabınının id'si).",
      "required": true
    },
    "DURATION_LIMIT": {
      "description": "Yükləmələr üçün ən çox səs vaxtı (minutes).",
      "required": true,
      "value": "55"
    }
  }
}
